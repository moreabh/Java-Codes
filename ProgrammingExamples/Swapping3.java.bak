import java.util.*;
class Swapping3
{
	public static void main(String[]args)
	{
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter first value(a):-");
		int a=sc.nextInt();
		System.out.print("Enter Second Value(b):-");
		int b=sc.nextInt();

	    a=a+b;//(4)(2)(6)
		b=a-b;//b=6-2=4
		a=a-b;//a=6-4=2

		System.out.println("a:"+a);
		System.out.print("b:"+b);
			
	}


}