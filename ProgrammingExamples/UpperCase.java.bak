import java.util.*;
class UpperCase
{
	public static void main(String[]args)
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a character:");
		char ch= sc.next().charAt(0);

		String cha=(ch>=65 && ch<=90)? ("Upper case"):("not upper case";)
		System.out.println(cha);
		
		   
	}

}