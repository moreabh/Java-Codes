import java.util.*;
class Digit
{
	public static void main(String[]args)
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a digit:");
		sc.nextLine();
		char ch= sc.nextLine().charAt(01);
		String cha=(ch>=48 && ch<=57)? ("is a digit"):("is not a digit");
		System.out.println(cha);
		
		   
	}

}