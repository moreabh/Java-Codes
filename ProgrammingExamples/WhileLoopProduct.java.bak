import java.util.*;
class WhileLoopProduct 
{
	public static void main(String[]args)
	{
	Scanner sc = new Scanner(System.in);
	System.out.println("Enter any Number: ");
	int num = sc.nextInt();
	int product = 1;
	
	
	while(num > 0)
		{
			int rem = num %10;
			product *= rem;
			num /= 10;
		}
		System.out.println("product of digits : "+product);

		/*for(;num>0;num/=10)
		{
		int rem = num%10;
		product =product*rem;
		
		}
		System.out.println("product of digits : "+product);*/

	}
}