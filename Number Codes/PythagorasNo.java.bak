//pythagoras Number
class PythagorasNo
{
	public static void main(String[]args)
	{                                                                                                                         
		System.out.println(isPythagoras(3,4,5));
	}									 
	public static boolean isPythagoras(int n1,int n2,int sum)
	{									 
		if ((n1*n1) + (n2*n2) ==(sum*sum))
		{								 
			return true ;				 
		}								 
		return false;					 
	}									 
}										 
// true