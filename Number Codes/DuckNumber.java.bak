//Duck Number 
import java.util.*;
class DuckNumber
{
	public static void main(String[]args)
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter a number:- ");
		String str = sc.next();

		if (str.charAt(0)=='0')
		{
			System.out.println(str+ "Is not a duck number");
		}
		else 
		{
			if (!str.contains("0"))
			{
				System.out.println(str+" Is not a duck number");
			}
			else 
			{
				System.out.println(str+" Is a duck number");
			}
		}
	}

}